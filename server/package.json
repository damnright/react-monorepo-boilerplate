{
  "name": "server",
  "version": "1.0.0",
  "packageManager": "pnpm@9.15.2",
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "start": "pnpm --filter common build && pnpm run build && cross-env NODE_ENV=production node dist/index.js",
    "build:alone": "tsc -p tsconfig.json",
    "build": "pnpm --filter common build && tsc -p tsconfig.json",
    "dev": "concurrently --raw \"pnpm --filter common watch\" \"cross-env NODE_ENV=development tsx watch src/index.ts\"",
    "dev:alone": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "clean": "rimraf dist node_modules",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",

    "@fastify/multipart": "^9.0.3",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.10.1",
    "@sentry/node": "^9.31.0",
    "@sentry/profiling-node": "^9.31.0",
    "@sinclair/typebox": "^0.34.37",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "common": "workspace:*",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "fastify": "^5.4.0",
    "fastify-plugin": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.10.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.18",
    "@types/node": "^24.0.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "jest": "^30.0.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "fastify",
    "api",
    "mongodb",
    "prisma",
    "typescript",
    "canvas",
    "aigc"
  ]
}